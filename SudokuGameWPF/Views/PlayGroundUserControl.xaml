        <Grid x:Name="WinGrid" Visibility="Hidden">
            <Rectangle VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Fill="#33AFB781"/>
            <Canvas x:Name="confettiCanvas" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>

            <Button x:Name="winButton" Content="Play Again" Style="{StaticResource RoundCorner}"
                    Width="250" Height="100" Click="PlayAgain_Click" Panel.ZIndex="5"
                    VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </Grid>
    </Grid>
</UserControl>
